{% include /get/htcondor_ce_releases.liquid %}

{% assign htcondor_ce_releases_old = "" | split : "" %}

{% for release in htcondor_ce_releases %}

    {% assign release_numbers = release.release_number | strip | split: "." %}

    {% if release_numbers[0] != "6"  %}
        {% assign htcondor_ce_releases_old = htcondor_ce_releases_old | push: release %}
    {% endif %}

{% endfor %}

{% assign htcondor_ce_releases_old = htcondor_ce_releases_old | sort: "sort_key" | reverse %}