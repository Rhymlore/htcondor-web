{% include /get/releases.liquid %}
{% assign releases = releases | where: "release_type", "htcondor-ce"%}

{% assign htcondor_ce_releases = "" | split : "" %}

{% for release in releases %}

    {% assign release_numbers = release.release_number | strip | split: "." %}

    {% assign htcondor_ce_releases = htcondor_ce_releases | push: release %}

{% endfor %}

{% assign htcondor_ce_releases = htcondor_ce_releases | sort: "sort_key" | reverse %}

