---
layout: blank
---
{% assign htcondor_releases = site.news | where: "release_type", "HTCondor" %}
{% assign release_array_releases = "" | split: "" %}
{% for news in htcondor_releases %}
{% assign release_string = news.release_number | prepend: "'" | append: "'" %}
{% assign release_array_releases = release_array_releases | push: release_string %}
{% endfor %}
{% assign htcondor_ce = site.news | where: "release_type", "HTCondor-CE" %}
{% assign release_array_ce = "" | split: "" %}
{% for news in htcondor_ce %}
{% assign release_string = news.release_number | prepend: "'" | append: "'" %}
{% assign release_array_ce = release_array_ce | push: release_string %}
{% endfor %}
{% assign htcondor_security = site.news | where: "release_type", "Security" %}
{% assign release_array_security = "" | split: "" %}
{% for news in htcondor_security %}
{% assign release_string = news.release_number | prepend: "'" | append: "'" %}
{% assign release_array_security = release_array_security | push: release_string %}
{% endfor %}
{
  "htcondor" : [{{ release_array_releases | join: ", " }}],
  "htcondor-ce" : [{{ release_array_ce | join: ", " }}],
  "security" : [{{ release_array_security | join: ", " }}]
}